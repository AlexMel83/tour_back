{
  "name": "interactive-tour-guide-backend",
  "description": "Інтерактивний туристичний гід Хмельниччини backend",
  "version": "1.0.0",
  "private": true,
  "main": "./back/server.js",
  "type": "module",
  "scripts": {
    "lint": "eslint . && prettier -w .",
    "test": "node --test .",
    "precommit": "eslint --config .eslintrc.json --fix \"**/*.{js,cjs,ts}\" --ignore-pattern \"**/*.spec.{js,cjs,ts}\" && prettier --write \"**/*.{js,cjs,ts,css}\"",
    "prepare": "husky install",
    "dev": "cross-env NODE_ENV=development nodemon ./server.js",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "db:clean": "cd ./sql/&& knex --knexfile knex migrate:rollback",
    "db:setup": "cd ./sql/ && knex migrate:latest && knex seed:run",
    "devdb:clean": "cd ./sql/ && cross-env NODE_ENV=development ./sql/knex migrate:rollback",
    "devdb:setup": "cd ./sql/ && cross-env NODE_ENV=development ./sql/knex migrate:latest && cross-env NODE_ENV=development knex seed:run",
    "knex": "knex --knexfile ./sql/knexfile.js"
  },
  "author": "AlexMel83",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/eslint-parser": "^7.24.6",
    "@eslint/js": "^9.9.0",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.27.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.7",
    "nodemon": "^3.0.1",
    "prettier": "^3.3.3"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "connect-pg-simple": "^10.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.18.1",
    "express-validator": "^7.0.1",
    "ipsp-js-sdk": "^2.0.11-alpha.1",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "liqpay": "^0.0.1",
    "moment-timezone": "^0.5.45",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "node-geocoder": "^4.3.0",
    "node-pg-migrate": "^7.5.2",
    "nodemailer": "^6.9.7",
    "openid-client": "^5.6.5",
    "pg": "^8.11.3",
    "set-value": "^4.1.0",
    "update": "^0.7.4",
    "uuid": "^10.0.0"
  }
}
